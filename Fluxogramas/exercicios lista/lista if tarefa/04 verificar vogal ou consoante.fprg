    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="quando o user digita upercase o condicional nao valida pos a variavel foideclarada como lowercase"/>
            <declare name="letra" type="String" array="False" size=""/>
            <output expression="&quot;Digite uma letra:&quot;" newline="True"/>
            <input variable="letra"/>
            <comment text="3.Fa&#231;a um Programa que verifique se uma letra digitada &#233; &quot;F&quot; ou &quot;M&quot; ou &#8220;O&#8221;. Conforme a letra escrever: F - Feminino, M - Masculino, O - Outro"/>
            <if expression="letra = &quot;m&quot;">
                <then>
                    <output expression="&quot; A letra digitada foi: &quot;&amp;letra&amp; &quot; Masculino&quot;" newline="True"/>
                </then>
                <else>
                    <if expression="letra = &quot;f&quot;">
                        <then>
                            <output expression="&quot; A letra digitada foi: &quot;&amp;letra&amp; &quot; Feminino&quot;" newline="True"/>
                        </then>
                        <else>
                            <if expression="letra = &quot;o&quot;">
                                <then>
                                    <output expression="&quot; A letra digitada foi: &quot;&amp;letra&amp; &quot; Outro&quot;" newline="True"/>
                                </then>
                                <else>
                                    <output expression="&quot; A letra digitada foi: &quot;&amp;letra&amp; &quot; N&#227;o foi a letra com palavra secreta&quot;" newline="True"/>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
